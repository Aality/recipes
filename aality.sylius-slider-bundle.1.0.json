{
    "manifests": {
        "aality/sylius-slider-bundle": {
            "manifest": {
                "bundles": {
                    "Aality\\SyliusSliderBundle\\SyliusSliderBundle": [
                        "all"
                    ]
                },
                "copy-from-package": {
                    "config/": "%CONFIG_DIR%/",
                    "assets/": "assets/"
                },
                "add-lines": [
                    {
                        "file": "assets/shop/entrypoint.js",
                        "content": "import './scss/vendor/aality-sylius-slider-bundle.scss';",
                        "position": "top",
                        "warn_if_missing": true
                    }
                ],
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "post-install-output": [
                    "<bg=green;fg=white>                                    </>",
                    "<bg=green;fg=white>         Congratulations !          </>",
                    "<bg=green;fg=white>   Thank you for using our bundle   </>",
                    "<bg=green;fg=white> Please read instructions in README </>",
                    "",
                    "<bg=blue;fg=white>             Visit our website at www.aality.fr              </>",
                    "<bg=blue;fg=white> Need some help ? We are available on Sylius Community Slack </>",
                    "<bg=blue;fg=white>     https://sylius-community.slack.com/team/U08H5A0PRRC     </>",
                    "<bg=blue;fg=white>     https://sylius-community.slack.com/team/U07CWPRNZGU     </>"
                ]
            },
            "files": [],
            "ref": "a3ac50b4ce3bce7d67a6c53684e5f77e121069c3"
        }
    }
}
