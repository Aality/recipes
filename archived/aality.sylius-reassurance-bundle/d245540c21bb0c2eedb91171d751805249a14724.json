                "bundles": {
                    "Aality\\ReassuranceBundle\\ReassuranceBundle": [
                        "all"
                    ]
                },
                "copy-from-package": {
                    "config/": "%CONFIG_DIR%/",
                    "assets/": "assets/"
                },
                "add-lines": [
                    {
                        "file": "assets/shop/entrypoint.js",
                        "content": "import './scss/vendor/aality-sylius-reassurance-bundle.scss';",
                        "position": "top",
                        "warn_if_missing": true
                    }
                ],
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "post-install-output": [
                    "<bg=blue;fg=white>                   </>",
                    "<bg=blue;fg=white> Congratulations ! </>",
                    "<bg=blue;fg=white>                   </>",
                    "",
                    "  * <fg=green>Run</> your application:",
                    "    1. If you use symfony/flex, you are done.",
                    "    2. If not, follow instructions from README file",
                    "",
                    "  * <fg=blue>Read</> the documentation at <comment>https://symfony.com/doc</>"
                ]
            },
            "files": [],
            "ref": "d245540c21bb0c2eedb91171d751805249a14724"
        }
    }
